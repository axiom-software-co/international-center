apiVersion: dapr.io/v1alpha1
kind: Component
metadata:
  name: ratelimit
spec:
  type: middleware.http.ratelimit
  version: v1
  metadata:
  - name: maxRequestsPerSecond
    value: "100"
  - name: headerKey
    value: "X-Real-IP"
  - name: statusCode
    value: "429"
  - name: responseMessage
    value: "Rate limit exceeded"